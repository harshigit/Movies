<h1>Lights-Camera-Action!!</h1>
<%= form_for :movie, url: movies_path do |f| %>
 
  
  <p> movietitle:<br/>
    <%= f.text_area :movietitle %>
  </p>
 
  <p> timings:<br/>
    <%= f.time_field :timings %>
  </p>
 
  <p> reviews:<br/>
    <%= f.text_area :reviews %> 
  </p>
  
   <p>
    <%= f.file_field :photo %>
  </p>
  
  <p>
    <%= f.submit "Add" %>
  </p>
<% end %>


<%=  link_to 'Back', movies_path %>