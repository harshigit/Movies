<%= form_for @movie, :html => { :multipart => true} do |f| %>

  <p>
    <%= f.label :movietitle %><br>
    <%= f.text_field :movietitle %>
  </p>
  
  <p>
    <%= f.label :timings %><br/>
    <%= f.time_field :timings %>
  </p>
 
  <p>
    <%= f.label :reviews %><br/>
    <%= f.text_area :reviews, :rows =>10 %>
  </p>
  
  <p>
    <%= f.file_field :photo %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>

<% end %>